{
    "t":{"label":"Read junction integration time",
         "send":["{jt:n}"]},
        "r":{"status":0, "ja":1.01}
}
# EOF
{
    "t":{"label":"Set EJ to Text mode",
         "send":["{ej:0}"]},
        "r":{"status":0, "ej":0}
}
